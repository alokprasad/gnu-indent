#: src/args.c:673
msgid ""
"usage: indent file [-o outfile ] [ options ]\n"
"       indent file1 file2 ... fileN [ options ]\n"
msgstr ""
"uporaba: indent datoteka [-o izlaznadatoteka ] [ opcije ]\n"
"         indent datoteka1 datoteka2 ... datotekaN [ opcije]\n"

