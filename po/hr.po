msgstr "%s: nepoznata opcija „%s”\n"

#: src/args.c:877
#, c-format
msgid "option: %s\n"
msgstr "opcija: %s\n"

#: src/args.c:892
#, c-format
msgid "GNU indent %s\n"
msgstr "GNU indent %s\n"

