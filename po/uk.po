#~ msgstr "Скінчилась пам'ять"

#~ msgid "Extra %c"
#~ msgstr "Зайвий %c"

#~ msgid "Unmatched #else"
#~ msgstr "#else без пари"

#~ msgid "Unmatched #elif"
#~ msgstr "#elif без пари"

